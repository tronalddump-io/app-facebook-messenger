<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

    <application>${appengineAppId}</application>
    <version>4</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
        <property name="facebook.verify_token" value="${facebookVerifyToken}" />
        <property name="facebook.access_token" value="${facebookAccessToken}" />
        <property name="facebook.app_secret" value="${facebookAppSecret}" />
    </system-properties>

</appengine-web-app>
